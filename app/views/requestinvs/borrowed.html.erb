<h1>Borrowed user requests for  <%= session[:user_name] %></h1>
<table class="styled-table">
  <thead>
    <tr>
      <th id="name_header">Name</th>
      <th id="number_header">Quantity to Issue</th>
      <th id="email_header">Email</th>
      <th id="team_name_header">Team Name</th>
      <th id="cost_per_item_header">Cost per Item</th>
      <th id="url_header">URL</th>
      <th id="issued_header">Issued?</th>
    </tr>
  </thead>
  <tbody>
    <% @requestinvs.each do |i| %>
      <tr>
        <td><%= i.name %></td>
        <td><%= i.number %></td>
        <td><%= i.email %></td>
        <td><%= i.team_name %></td>
        <td><%= i.cost_per_item %></td>
        <td><%= i.url %></td>
        <td><%= i.issued %></td>
      </tr>
    <% end %>
  </tbody>
</table>